<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
        ">

    <changeSet id="kpo-core-testdata" author="klenkes74">
        <comment>Load default tenant.</comment>

        <loadData tableName="identities" file="data/IDENTITIES-test.csv"/>
        <loadData tableName="accounts" file="data/ACCOUNTS-test.csv"/>
        <loadData tableName="roles" file="data/ROLES-test.csv"/>
        <loadData tableName="accounts_roles" file="data/ACCOUNTS_ROLES-test.csv"/>
        <loadData tableName="tickets" file="data/TICETS-test.csv"/>
    </changeSet>


    <changeSet id="kpo-tag-core-testdata" author="klenkes74">
        <tagDatabase tag="kpo-core-testdata-loaded"/>
    </changeSet>
</databaseChangeLog>
