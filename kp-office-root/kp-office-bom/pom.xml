<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>de.kaiserpfalz-edv.office.base</groupId>
    <artifactId>kp-office-bom</artifactId>
    <packaging>pom</packaging>
    <version>1.0.0-SNAPSHOT</version>

    <name>KP Office :: Bill Of Materials</name>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>maven-deploy-plugin</artifactId>
                    <version>2.8.2</version>
                </plugin>

                <plugin>
                    <artifactId>maven-install-plugin</artifactId>
                    <version>2.5.2</version>
                </plugin>

                <plugin>
                    <artifactId>maven-clean-plugin</artifactId>
                    <version>3.0.0</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-contacts</artifactId>
                <version>${project.version}</version>
                <type>ejb</type>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-contacts</artifactId>
                <version>${project.version}</version>
                <type>ejb-client</type>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-contacts</artifactId>
                <version>${project.version}</version>
                <classifier>api</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-contacts</artifactId>
                <version>${project.version}</version>
                <classifier>jpa</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>

            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-files</artifactId>
                <version>${project.version}</version>
                <type>ejb</type>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-files</artifactId>
                <version>${project.version}</version>
                <classifier>api</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-files</artifactId>
                <version>${project.version}</version>
                <classifier>jpa</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-files</artifactId>
                <version>${project.version}</version>
                <type>ejb-client</type>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>

            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-files</artifactId>
                <version>${project.version}</version>
                <type>ejb</type>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-dms</artifactId>
                <version>${project.version}</version>
                <classifier>api</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-dms</artifactId>
                <version>${project.version}</version>
                <classifier>jpa</classifier>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>de.kaiserpfalz-edv.office.base</groupId>
                <artifactId>kp-office-dms</artifactId>
                <version>${project.version}</version>
                <type>ejb-client</type>
                <exclusions>
                    <exclusion>
                        <groupId>de.kaiserpfalz-edv.commons</groupId>
                        <artifactId>kp-commons-ejb</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>


            <dependency>
                <groupId>de.kaiserpfalz-edv.office</groupId>
                <artifactId>kp-office</artifactId>
                <version>${project.version}</version>
                <type>ear</type>
            </dependency>
        </dependencies>
    </dependencyManagement>
</project>
